{
  "ecma": 5,
  "safari10": true,
  "compress": {
    "defaults": false,

    "booleans": true,
    "collapse_vars": true,
    "comparisons": true,
    "computed_props": true,
    "conditionals": true,
    "dead_code": true,
    "directives": true,
    "evaluate": true,
    "hoist_funs": true,
    "hoist_props": true,
    "if_return": true,
    "join_vars": true,
    "keep_fargs": true,
    "loops": true,
    "negate_iife": true,
    "properties": true,
    "reduce_vars": true,
    "sequences": true,
    "side_effects": true,
    "switches": true,
    "unsafe_comps": true,

    "passes": 2,

    "inline": true,
    "typeofs": false,

    "pure_funcs": [
      "isObject", "isFunction",
      "hasOwnProperty", "createMap",
      "getFunctionName", "createErrorMessage"
    ]
  }
}
